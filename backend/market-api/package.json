{
  "name": "@market/market-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "prod": "node ./dist/index.js",
    "devwin": "nodemon --exec npx ts-node src/index.ts",
    "typeorm": "tyoeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/ormconfig.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/ormconfig.ts",
    "migration:show": "npm run typeorm migration:show -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate -d ./src/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create",
    "seed": "nodemon --exec npx ts-node src/seed-script/seed-script.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
